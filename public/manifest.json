{
    "manifest_version": 3,
    "name": "kinopoisk-break",
    "version": "1",
    "description": "Добавляет кнопку на фильме кинопоиска для перехода к просмотру",
    "content_scripts": [
        {
            "matches": ["https://www.kinopoisk.ru/*"],
            "js": ["integrateScript.js"]
        }
    ],
    "icons": {
        "16": "img/favicon-16x16.png",
        "32": "img/favicon-32x32.png",
        "48": "img/chrome-192x192.png",
        "128": "img/chrome-192x192.png"
    }
}
